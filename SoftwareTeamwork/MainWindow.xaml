<local:AIWindow x:Class="SoftwareTeamwork.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SoftwareTeamwork"
        mc:Ignorable="d"  Title="MainWindow" Background="{DynamicResource BackgroundBrush}">
    <local:AIWindow.AreaIcon>
        <local:DAreaIcon AreaVisibility="True">
            <local:DAreaIcon.Dcontextmenu>
                <local:DContextMenu Style="{DynamicResource DcontextMenu}"/>
            </local:DAreaIcon.Dcontextmenu>
            <local:DAreaIcon.FlowIconPopup>
                <local:FlowTrendPopups Style="{DynamicResource FlowAnaPanel}"/> 
            </local:DAreaIcon.FlowIconPopup>
        </local:DAreaIcon>
    </local:AIWindow.AreaIcon>
    <Grid Margin="2">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="auto" />
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>
        <Grid Width="100">
            
        </Grid>
        <Frame>
            
        </Frame>
    </Grid>
</local:AIWindow>
